<script type="text/javascript">
  $(document).ready(function() {
    /*--------Course preview filter ----------*/
    $(".courseh-link").click(function()
    {
        const value=$(this).attr('data-filter');
        if(value=="all")
        {
            $(".cardsc-item").show('1000');
        }
        else
        {
            $(".cardsc-item").not('.'+value).hide('1000');
            $(".cardsc-item").filter('.'+value).show('1000');
        }

    });



    /*-------Teachers List -----*/

    var itemCount=3;
    if ($(window).width() < 700){
        itemCount=1;
        
    }
    else if($(window).width() < 1200)
    {
        itemCount=2;
    }
    else {
       itemCount=3;
   }
   var autoplaySlider = $('#autoplay').lightSlider({
       item:itemCount,
       auto:true,
       loop:true,
       
       onBeforeSlide: function (el) {
        $('#current').text(el.getCurrentSlideCount());
    } 
});
   $('#total').text(autoplaySlider.getTotalSlideCount());

});
//lower interval for testings//$('.carousel').carousel({//  interval: 2000//})
//$('#myCarousel').on('slide.bs.carousel', function () {//  $("#myCarousel").carousel('pause');//})
//.carousel('cycle')


</script>
